{
  "name": "yourmanager",
  "version": "1.0.0",
  "description": "",
  "boot": "./dist/boot.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"node server/index.js\" ",
    "lite": "lite-server",
    "lite-server-test": "lite-server --config=liteserver-test-config.json",
    "test": "tsc && concurrently \"npm run tsc:w\" \"npm run lite-server-test\" ",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build": "tsc",
    "rollup": "rollup -f iife -c -o dist/prod/client/app/bundle.es2015.js",
    "es5": "tsc --target es5 --allowJs dist/prod/client/app/bundle.es2015.js --out dist/prod/client/app/bundle.js",
    "minify": "uglifyjs dist/prod/client/app/bundle.js --screw-ie8 --compress --mangle --output dist/prod/client/app/bundle.min.js",
    "ngc": "ngc -p . && cp client/app/* client/compiled",
    "clean": "rm -rf dist && rm -rf client/app/*.ngfactory.ts && cd client/compiled && ls | grep -v client/boot-ngc.ts | xargs rm && cd ..",
    "build_prod": "npm run rollup && npm run es5 && npm run minify"
  },
  "keywords": [
    "your",
    "manager"
  ],
  "author": "Sumit khanduri",
  "license": "ISC",
